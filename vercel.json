{
  "version": 2,
  "builds": [
    {
      "src": "server-vercel.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server-vercel.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/daily-sync",
      "schedule": "0 11 * * *"
    }
  ]
}