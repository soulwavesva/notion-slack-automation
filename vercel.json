{
  "version": 2,
  "builds": [
    {
      "src": "server-vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server-vercel.js"
    }
  ],
  "functions": {
    "server-vercel.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/cron/check-completed",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/cron/check-new",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 11,14,14,18 * * *"
    },
    {
      "path": "/api/cron/sync",
      "schedule": "0 11,13,15,17,19,21,23,1,3 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}